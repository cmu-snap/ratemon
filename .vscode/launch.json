{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug unfair_runtime.py",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/unfair/runtime/unfair_runtime.py",
            // "console": "integratedTerminal",
            "console": "integratedTerminal",
            "args": ["-h"],
            "autoReload": {
                "enable": false
            },
            "redirectOutput": true,
            "env": {
                "PYTHONPATH": "${workspaceFolder}"
            },
            "sudo": true
        },
        {
            "name": "Run unfair_runtime.py",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/unfair/runtime/unfair_runtime.py",
            // "console": "integratedTerminal",
            "console": "integratedTerminal",
            "args": ["--interval-ms=100", "--limit=100", "--disable-inference", "--model=HistGbdtSklearn", "--model-file=${workspaceFolder}/data/models/gbdt.pickle"],
            "autoReload": {
                "enable": false
            },
            "redirectOutput": true,
            "env": {
                "PYTHONPATH": "${workspaceFolder}"
            },
            "sudo": true
        }
    ]
}