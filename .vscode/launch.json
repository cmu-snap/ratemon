            "redirectOutput": true,
            "env": {
                "PYTHONPATH": "${workspaceFolder}"
            },
            "sudo": true
        },
        {
            "name": "(r) runtime, schedule, cprofile",
            "type": "python",
            "request": "launch",
            "module": "cProfile",
            "console": "integratedTerminal",
            "args": [
                "-o", "/tmp/tmp.prof",
                "${workspaceFolder}/unfair/runtime/unfair_runtime.py",
                "--model=HistGbdtSklearn",
                "--model-file=${workspaceFolder}/test/HistGbdtSklearn.pickle",
                "--interface=ens3",
                "--reaction-strategy=file",
                "--schedule=${workspaceFolder}/test/rwnd_schedule_static.csv",
                "--cgroup=${workspaceFolder}/test/test_cgroup",
                "--inference-interval-ms=2000",
                "--disable-inference"
            ],
            "autoReload": {
                "enable": false
            },
            "redirectOutput": true,
            "env": {
                "PYTHONPATH": "${workspaceFolder}"
            },
            "sudo": true
        },
        {
            "name": "(r) train cubic cubic",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/unfair/model/train.py",
            "console": "integratedTerminal",
            "args": [
                "--no-rand",
                "--conf-trials=1",
                "--model=HistGbdtSklearn",
                "--balance",
                "--drop-popular",
                "--analyze-features",
                "--clusters=20",
                "--features-to-pick=10",
                "--permutation-importance-repeats=2",
                "--sample-percent=100",
                "--out-dir=/home/ccanel/fawnstore2/out/cloudlab/2021-5-12/cubic-cubic",
